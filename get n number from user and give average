""""

n=int(input("kac adet sayi girilicek ?"))
a=[]

for i in range(0,n):
    elem=int(input("sayilari giriniz :"))
    a.append(elem)
    avg=sum(a)/n
    break
    
    print("girilen sayilarin ortalamasi :",round(avg,2))


